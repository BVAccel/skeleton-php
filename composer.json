{
    "name": "bvaccel/:package_name",
    "description": ":package_description",
    "keywords": [
        "bvaccel",
        ":package_name"
    ],
    "homepage": "https://github.com/bvaccel/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "fullstack@bvaccel.com",
            "homepage": "https://bvaccel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "BVAccel\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BVAccel\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BVAccel\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "BVAccel\\Skeleton\\SkeletonFacade"
            }
        }
    }
}
